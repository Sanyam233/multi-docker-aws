{
  "AWSEBDockerrunVersion": 1,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "sanyamgupta23/multi-container-aws-client",
      "essential": false
    },
    {
      "name": "worker",
      "image": "sanyamgupta23/multi-container-aws-worker",
      "essential": false
    },
    {
      "name": "server",
      "image": "sanyamgupta23/multi-container-aws-server",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "sanyamgupta23/multi-container-aws-nginx",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
